{
  "name": "donate-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dangerously": "ng serve --host 0.0.0.0",
    "test": "ng test",
    "test-ci": "ng test --karma-config ./karma-ci.conf.js --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --deploy-url=/d/",
    "build:staging": "ng build --deploy-url=/d/ --prod --configuration=staging",
    "build:regression": "ng build --deploy-url=/d/ --prod --configuration=regression",
    "build:production": "ng build --deploy-url=/d/ --prod",
    "build:ssr:production": "npm run build:production && ng run donate-frontend:server:production",
    "build:ssr:regression": "npm run build:regression && ng run donate-frontend:server:regression",
    "build:ssr:staging": "npm run build:staging    && ng run donate-frontend:server:staging",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "ci": "npm run lint && npm run test-ci && npm run e2e-ci && npm run build:staging",
    "dev:ssr": "ng run donate-frontend:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "prerender": "ng run donate-frontend:prerender"
  },
  "dependencies": {
    "@angular/animations": "^10.1.1",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "^10.1.1",
    "@angular/compiler": "^10.1.1",
    "@angular/core": "^10.1.1",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.1.1",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "^10.1.1",
    "@angular/platform-browser-dynamic": "^10.1.1",
    "@angular/platform-server": "^10.1.1",
    "@angular/router": "^10.1.1",
    "@nguniversal/express-engine": "^10.1.0",
    "@stripe/stripe-js": "^1.9.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "material-icons-font": "^2.1.0",
    "morgan": "^1.10.0",
    "ngx-infinite-scroll": "^8.0.2",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.8",
    "@angular/cli": "^10.1.1",
    "@angular/compiler-cli": "^10.1.1",
    "@angular/language-service": "^10.1.1",
    "@nguniversal/builders": "^10.1.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.45",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.10.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "puppeteer": "^5.3.1",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "^6.1.3",
    "typescript": "~3.9.7",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}

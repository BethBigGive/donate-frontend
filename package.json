{
  "name": "donate-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dangerously": "ng serve --host 0.0.0.0",
    "test": "ng test",
    "test-ci": "ng test --karma-config ./karma-ci.conf.js --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server:production": "webpack --config webpack.server.production.config.js --progress --colors",
    "compile:server:regression": "webpack --config webpack.server.regression.config.js --progress --colors",
    "compile:server:staging": "webpack --config webpack.server.staging.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build": "ng build --deploy-url=/d/",
    "build:staging": "ng build --deploy-url=/d/ --prod --configuration=staging",
    "build:regression": "ng build --deploy-url=/d/ --prod --configuration=regression",
    "build:production": "ng build --deploy-url=/d/ --prod",
    "build:ssr:production": "npm run build:client-and-server-bundles:production && npm run compile:server:production",
    "build:ssr:regression": "npm run build:client-and-server-bundles:regression && npm run compile:server:regression",
    "build:ssr:staging": "npm run build:client-and-server-bundles:staging && npm run compile:server:staging",
    "build:client-and-server-bundles:production": "npm run build:production && ng run donate-frontend:server:production",
    "build:client-and-server-bundles:regression": "npm run build:regression && ng run donate-frontend:server:regression",
    "build:client-and-server-bundles:staging": "npm run build:staging && ng run donate-frontend:server:staging",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "ci": "npm run lint && npm run test-ci && npm run e2e-ci && npm run build:staging"
  },
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.2",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/platform-server": "^9.0.2",
    "@angular/router": "^9.0.2",
    "@nguniversal/express-engine": "^9.0.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "material-icons-font": "^2.0.0",
    "morgan": "^1.9.1",
    "ngx-infinite-scroll": "^8.0.1",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "^9.0.3",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "^9.0.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "^2.0.8",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "^5.4.3",
    "puppeteer": "2.1.*",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.7.5",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
